            <name>LMS-ejb</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <included-library dirs="200">file.reference.hibernate-3.3.2.GA.jar</included-library>
            <included-library dirs="200">file.reference.commons-io-1.3.2.jar</included-library>
            <included-library dirs="200">file.reference.commons-lang-2.3.jar</included-library>
            <included-library dirs="200">file.reference.dom4j-1.6.1.jar</included-library>
            <included-library dirs="200">file.reference.ejb3-persistence.jar</included-library>
            <included-library dirs="200">file.reference.hibernate-annotations.jar</included-library>
            <included-library dirs="200">file.reference.hibernate-entitymanager-3.3.2.GA.jar</included-library>
            <included-library dirs="200">file.reference.hibernate-commons-annotations.jar</included-library>
            <included-library dirs="200">file.reference.hibernate-validator.jar</included-library>
