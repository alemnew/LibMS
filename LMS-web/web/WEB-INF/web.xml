<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <filter>
        <filter-name>struts2</filter-name>
        <filter-class>org.apache.struts2.dispatcher.FilterDispatcher</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>struts2</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <ejb-ref>
        <ejb-ref-name>StaffsBean</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>org.mk.rc.intf.StaffsBeanRemote</remote>
    </ejb-ref>
    <ejb-ref>
        <ejb-ref-name>PublicationBean</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>org.mk.rc.intf.PublicationBeanRemote</remote>
    </ejb-ref>
    <ejb-ref>
        <ejb-ref-name>UsersBean</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>org.mk.rc.intf.UsersBeanRemote</remote>
    </ejb-ref>
    <ejb-ref>
        <ejb-ref-name>ReservationBean</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>org.mk.rc.intf.ReservationBeanRemote</remote>
    </ejb-ref>
    <ejb-ref>
        <ejb-ref-name>LoanBean</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>org.mk.rc.intf.LoanBeanRemote</remote>
    </ejb-ref>
    <ejb-ref>
        <ejb-ref-name>FavoriteListBean</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>org.mk.rc.intf.FavoriteListBeanRemote</remote>
    </ejb-ref>
    <ejb-ref>
        <ejb-ref-name>UsersBean1</ejb-ref-name>
        <ejb-ref-type>Session</ejb-ref-type>
        <remote>org.mk.rc.intf.UsersBeanRemote</remote>
    </ejb-ref>
</web-app>
